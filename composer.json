{
    "name": "mvd81/laravel-logreader",
    "description": "A Laravel package to read and browse log files via a Sanctum-authenticated API",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "logs",
        "log-reader",
        "log-viewer"
    ],
    "authors": [
        {
            "name": "MVD"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Mvd81\\LaravelLogreader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mvd81\\LaravelLogreader\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mvd81\\LaravelLogreader\\LogreaderServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
